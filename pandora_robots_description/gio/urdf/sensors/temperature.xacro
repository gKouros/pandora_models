<?xml version="1.0"?>

<robot xmlns:xacro="http://ros.org/wiki/xacro" >

  <xacro:include filename="$(find pandora_sensors_description)/temperature/urdf/*.urdf.xacro" />
  
  <xacro:macro name="temperature" >
    <xacro:panasonic_grid_eye parent="kinect_link"
                              prefix_y="middle" >
      <origin xyz="0.0173 0.0949 0.0"
              rpy="0 0 0" />
    </xacro:panasonic_grid_eye>
  </xacro:macro>

</robot>
