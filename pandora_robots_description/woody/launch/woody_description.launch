<?xml version="1.0"?>

<launch>

  <param name="robot_description"
         command="$(find xacro)/xacro.py '$(find pandora_robots_description)/woody/urdf/woody.urdf.xacro'" />

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" >
    <rosparam command="load" file="$(find pandora_robots_description)/woody/config/joint_states.yaml" />
  </node>

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />

</launch>
