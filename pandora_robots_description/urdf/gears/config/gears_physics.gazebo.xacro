<!-- Joints (URDF) ######################################################## -->

  <!-- ==================================================================== -->
  <xacro:macro name="linear_elevator_joint_urdf_physics" >

    <axis xyz="0 0 1" />

    <!-- <calibration rising="0.0"
                      falling="0.0" /> -->

    <!-- <dynamics damping="0.0"
                   friction="0.0" /> --> 

    <limit lower="${linear_elevator_joint_min_limit}"
           upper="${linear_elevator_joint_max_limit}"
           effort="1000.0"
           velocity="1000.0" />

    <!-- <safety_controller soft_lower_limit="0.0"
                            soft_upper_limit="0.0"
                            k_position="0.0"
                            k_velocity="0.0" /> -->

  </xacro:macro>
