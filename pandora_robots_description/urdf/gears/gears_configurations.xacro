<?xml version="1.0"?>

<robot xmlns:xacro="http://ros.org/wiki/xacro" >
  
<!-- Elements Status ###################################################### -->

  <!-- Links ============================================================== -->
  <xacro:property name="enable_base_link" value="true" />
  <xacro:property name="enable_side_links" value="true" />
  <xacro:property name="enable_wheel_links" value="true" />
  <xacro:property name="enable_linear_links" value="true" />
